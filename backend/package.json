{
  "name": "rbac-configuration-tool-backend",
  "version": "1.0.0",
  "description": "Backend API for RBAC Configuration Tool",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:render": "npx prisma generate && tsc",
    "start": "node dist/index.js",
    "setup": "tsx scripts/setup.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/setup.ts",
    "postinstall": "npx prisma generate || echo 'Prisma generate failed, continuing...'"
  },
  "keywords": [
    "rbac",
    "authentication",
    "authorization",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.9",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
